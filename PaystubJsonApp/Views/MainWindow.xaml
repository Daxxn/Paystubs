<Window x:Class="PaystubJsonApp.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PaystubJsonApp"
		xmlns:views="clr-namespace:PaystubJsonApp.Views"
        mc:Ignorable="d"
        Title="Paystubs" Height="900" Width="800">
	<TabControl
		x:Name="TabControl"
		TabStripPlacement="Top"
	>
		<TabItem Header="Paystubs" x:Name="PaystubsTab">
			<ContentControl x:Name="PaystubControl"/>
		</TabItem>
		<TabItem Header="Repair Orders" x:Name="RepairOrdersTab">
			<ContentControl x:Name="RepairOrderControl"/>
		</TabItem>
		<TabItem Header="Work Orders" Name="WorkOrdersTab">
			<ContentControl x:Name="WorkOrderControl"/>
		</TabItem>
	</TabControl>
</Window>
